<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.probook.proyecto.Menu">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <Spinner
                    android:id="@+id/spinnerUniremington"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerSedes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerProgramas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerAdmisiones"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerPlataformas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerBienesyegre"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerComunidad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
                <Spinner
                    android:id="@+id/spinnerUbicacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </Spinner>
            </LinearLayout>
        </ScrollView>
        <WebView
            android:layout_width="match_parent"
            android:layout_height="420dp"
            >
        </WebView>
        <Spinner
            android:id="@+id/web"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></Spinner>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
